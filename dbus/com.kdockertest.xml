<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
        "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">

<node>
    <interface name="com.kdockertest">
        <method name="exec">
            <arg name="argv" type="s" direction="in" />
            <arg name="reply" type="s" direction="out" />
        </method>
        <signal name="docking">
            <arg name="appWindowId" type="u" direction="out"/>
        </signal>
        <signal name="undocking">
            <arg name="appWindowId" type="u" direction="out"/>
        </signal>
    </interface>
</node>
